{
   "data": {
      "paths": {
         "refdir": "",
         "primarydir": ""
      },
      "columns": {
         "primary_dt_pos": 0,
         "primary_wl_pos": 1,
         "ref_dt_pos": 0,
         "ref_wl_pos": 1
      }
   },

  "analysis": {
      "mode": "raw",
      "years": ["all_years"]
  },

   "output": {
      "base_filename": "",
      "path": "generated_files",

      "generate_reports_for_years": {
         "metrics_summary": [],
         "vertical_offset_info_duration_filtered": [],
         "vertical_offset_info_value_filtered": [],
         "temporal_shifts_summary": [],
         "annotated_raw_data": []
     }
   },

   "logging": {
      "enabled": true,
      "level": "INFO",
      "file": "app.log"
   },

  "filter_offsets_by_duration": {
      "threshold": "0 days",
      "type": "min",
      "is_strict": false,
      "nonzero": false
  },

  "filter_offsets_by_value": {
      "threshold": 0.0,
      "use_abs": true,
      "type": "min",
      "is_strict": false,
      "nonzero": false
  },

  "filter_gaps_by_duration": {
      "threshold": "0 days",
      "type": "min",
      "is_strict": false
  },

  "temporal_shift_correction": {
      "number_of_intervals": 0,
      "replace_with_nans": true
  }
}